{
	"name": "linux-kernel-develop",
	"image": "linux-kernel-develop:develop",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"runArgs": [
		"--name",
		"linux-kernel-develop"
	],
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-vscode.cpptools",
				"ms-vscode.makefile-tools"
			]
		}
	},
	"postCreateCommand": "uname -a"
}
